{
  "name": "hello-typescript",
  "version": "1.0.0",
  "description": "Type:",
  "type": "module",
  "scripts": {
    "clean": "npm run rmdir dist",
    "test": "npm run test:jest; npm run test:vitest",
    "test:jest": "jest",
    "test:vitest": "vitest run",
    "coverage": "vitest run --coverage",
    "rmdir": "node -e \"var fs = require('fs'); try{process.argv.slice(1).map((fpath) => fs.rmdirSync(fpath, { recursive: true }))}catch(err){console.log(err)}; process.exit(0);\"",
    "build": "npm run clean && tsc",
    "build:tsc": "tsc",
    "build:tsup": "tsup",
    "format": "biome format --write .",
    "format:check": "biome format ."
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@eslint/core": "^0.6.0",
    "@eslint/js": "^9.9.0",
    "@types/eslint__js": "^8.42.3",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.5.5",
    "@vitest/coverage-v8": "^2.0.5",
    "date-fns": "^4.1.0",
    "eslint": "^9.9.0",
    "eslint-plugin-vitest": "^0.5.4",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0",
    "vitest": "*"
  }
}
