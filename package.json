{
  "name": "hello-typescript",
  "version": "1.0.0",
  "description": "Type:",
  "type": "module",
  "scripts": {
    "clean": "npm run rmdir dist",
    "lint": "eslint .",
    "test": "npm run test:jest; npm run test:vitest",
    "test:jest": "jest",
    "test:vitest": "vitest run",
    "coverage": "vitest run --coverage",
    "rmdir": "node -e \"var fs = require('fs'); try{process.argv.slice(1).map((fpath) => fs.rmdirSync(fpath, { recursive: true }))}catch(err){console.log(err)}; process.exit(0);\"",
    "build": "npm run clean && tsc",
    "build:tsc": "tsc",
    "build:tsup": "tsup"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@vitest/coverage-v8": "^2.1.6",
    "date-fns": "^4.1.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "npm-check-updates": "^17.1.11",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "*"
  }
}
